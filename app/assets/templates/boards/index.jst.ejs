<div class="row">
  <% boards.each(function (board) { %>
    <div id="draggable" class="col-xs-6 col-md-4 col-lg-2 pin-or-board-space">
      <img src="<%=board.escape("image_url")%>"></img>
      <p class="text-center"><%= board.escape("name") %></p>
    </div>
  <% }); %>
</div>
  
<% $(".pin-or-board-space").css({"margin":10 + "px", "padding":10 + "px"}) %>